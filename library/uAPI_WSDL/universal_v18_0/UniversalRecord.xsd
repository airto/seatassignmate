    <xs:element name="ProviderReservationSearchModifiers">
        <xs:annotation>
            <xs:documentation>
                Controls and switches for the PNR Search request.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="IncludeAllNames" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        If true, include all the passenger names in the
                        results. Otherwise include only the name of the
                        primary passenger.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="IncludeAgentInfo" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Include information about the agent who created,
                        modified or ticketed the PNR.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MaxResults" use="optional" type="common:typeMaxResults" default="20"/>
            <xs:attribute name="StartFromResult" use="optional" type="common:typeStartFromResult"
                default="1"/>
            <xs:attribute name="ExcludeAir" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Air reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExcludeVehicle" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Vehicle reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExcludeHotel" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Hotel reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExcludeGround" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Ground reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

