                <xs:element ref="common:VendorLocation" minOccurs="0" maxOccurs="unbounded"/>
                
                <xs:choice>
                    <xs:element ref="hotel_v19_0:HotelProperty" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="hotel_v19_0:HotelStay" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="hotel_v19_0:HotelRulesModifiers" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="hotel_v19_0:HotelDetailsModifiers" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="hotel_v19_0:HotelRateDetail" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="hotel_v19_0:PromotionCode" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <!-- Rail related information -->               
                <xs:choice>
                    <xs:element ref="rail_v14_0:RailPricingSolution" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="rail_v14_0:RailFareNoteList" minOccurs="0"/>
