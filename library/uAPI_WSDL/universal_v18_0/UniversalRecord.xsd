            <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="optional">
                <xs:annotation>
                    <xs:documentation>Alpha numeric String to identify a SavedTrip uniquely.</xs:documentation>
                </xs:annotation>                
            </xs:attribute>
            <xs:attribute name="UniversalRecordLocatorCode" type="common:typeLocatorCode" use="optional">
                <xs:annotation>
                    <xs:documentation>Alpha numeric String to identify a the UniversalRecord created using this SavedTrip.</xs:documentation>
                </xs:annotation>                
            </xs:attribute>            
            <xs:attribute name="Name"  use="required">
                <xs:annotation>
                    <xs:documentation>Name of the Saved Trip.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
