            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="SavedTripActivity">
        <xs:annotation>
            <xs:documentation>This element ties together related objects for a saved trip.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="BookingTravelerRef" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Reference to the associated travelers.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="Key" type="common:typeRef" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="SearchPassengerRef" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Reference to the associated travelers.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="Key" type="common:typeRef" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PointOfSaleRef" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Reference to the point of sale present in the saved trip.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="Key" type="common:typeRef" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="AccountingRemarkRef" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation>Reference to the AccountingRemark present in the saved trip.</xs:documentation>
                    </xs:annotation>
