<?xml version="1.0" encoding="UTF-8"?>
<xs:schema  xmlns="http://www.travelport.com/schema/rail_v14_0" 
            xmlns:xs="http://www.w3.org/2001/XMLSchema" 
            targetNamespace="http://www.travelport.com/schema/rail_v14_0"
            xmlns:common="http://www.travelport.com/schema/common_v17_0"
            xmlns:universalRecord_v18_0="http://www.travelport.com/schema/universal_v18_0"
            elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../common_v17_0/CommonReqRsp.xsd" namespace="http://www.travelport.com/schema/common_v17_0"/>
    <xs:import schemaLocation="../universal_v18_0/UniversalRecord.xsd" namespace="http://www.travelport.com/schema/universal_v18_0"/>
    <xs:include schemaLocation="Rail.xsd" />

    <xs:element name="RailAvailabilitySearchReq">
        <xs:annotation>
            <xs:documentation>Queries the host for availability.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="SearchRailLeg" maxOccurs="9"/>
                        <xs:element ref="common:SearchPassenger" maxOccurs="9"/>
                        <xs:element ref="RailSearchModifiers" minOccurs="0" />
                         <xs:element ref="RailPricingModifiers" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="ResponseType" type="typeResponseType" use="optional">
			            <xs:annotation>
			                <xs:documentation>Indicates the type of information that should be returned in RailShopAPIResponse(Schedules/Availability).If not sent, type Availability will be requested.</xs:documentation>
			            </xs:annotation>
			        </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="RailAvailabilitySearchRsp">
        <xs:annotation>
            <xs:documentation>Returns the result of an availability search on host.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="RailSegmentList" minOccurs="0" />
                        <xs:element ref="RailJourneyList" minOccurs="0"/>
                        <xs:element ref="RailPricingSolution" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="RailFareNoteList" minOccurs="0" />
                        <xs:element ref="RailFareIDList" minOccurs="0" />
                        <xs:element ref="RailFareList" minOccurs="0" />
                        <xs:element ref="common:HostTokenList" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="ResponseType" type="typeResponseType" use="optional">
                        <xs:annotation>
                            <xs:documentation>Indicates the type of information returned in RailShopAPIResponse(Schedules/Availability).</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailCreateReservationReq">
        <xs:annotation>
            <xs:documentation>Creates a rail reservation with the host.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseCreateReservationReq">
                    <xs:sequence>
                        <xs:element ref="RailPricingSolution" />
                        <xs:element ref="common:FormOfPayment" minOccurs="0" />
                        <xs:element ref="common:Payment" minOccurs="0" />
						<xs:element ref="RailFareNoteList" minOccurs="0">
						    <xs:annotation>
                        		<xs:documentation>List of RailFareNote(s) that is referenced by key in RailFare.</xs:documentation>
                        	</xs:annotation>
                        </xs:element>                    
                        <xs:element ref="common:HostTokenList" minOccurs="0" />
                        <xs:element ref="RailAutoSeatAssignment" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:complexType name="typeRailReservationRsp" abstract="true">
        <xs:complexContent>
            <xs:extension base="common:BaseRsp">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="universalRecord_v18_0:UniversalRecord" minOccurs="0" />
                    </xs:choice>
                </xs:sequence> 
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="typeRailCreateReservationRsp" abstract="true">
        <xs:complexContent>
            <xs:extension base="common:BaseRsp">
                <xs:sequence>
                    <xs:choice>
                        <xs:element ref="universalRecord_v18_0:UniversalRecord" minOccurs="0" />
                    </xs:choice>
                        <xs:element ref="RailSolutionChangedInfo" minOccurs="0" maxOccurs="unbounded"/>     
                </xs:sequence> 
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:element name="RailCreateReservationRsp">
        <xs:annotation>
            <xs:documentation>Returns rail reservation information with ticketing info etc..</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeRailCreateReservationRsp" /> 
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailExchangeRsp">
        <xs:annotation>
            <xs:documentation>Returns rail exchange reservation information with ticketing/refund info etc..</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="typeRailReservationRsp"/>                    
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
 
  <xs:element name="RailRefundQuoteReq">
        <xs:annotation>
            <xs:documentation>Request to quote a refund for an itinerary</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required">
                        <xs:annotation>
                            <xs:documentation>The unique identifier for this rail reservation</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailRefundQuoteRsp">
        <xs:annotation>
            <xs:documentation>Returns rail refund information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="RailRefundInfo" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
     <xs:element name="RailRefundReq">
        <xs:annotation>
            <xs:documentation>Request to cancel the booking</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:ContinuityCheckOverride" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>This element will be used if user wants to override segment continuity check.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="common:FormOfPayment" minOccurs="0" />
                    </xs:sequence>
			        <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required">
			            <xs:annotation>
			                <xs:documentation>The unique identifier for this rail reservation</xs:documentation>
			            </xs:annotation>
			        </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailRefundRsp">
        <xs:annotation>
            <xs:documentation>Returns rail cancel information.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="common:Payment" minOccurs="0" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="RailExchangeQuoteReq">
        <xs:annotation>
            <xs:documentation>Queries the host for availability.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="SearchRailLeg" maxOccurs="9"/>
                        <xs:element ref="RailSearchModifiers" minOccurs="0" />
                        <xs:element ref="common:SearchPassenger" minOccurs="0" maxOccurs="9"/>
                    </xs:sequence>
                    <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required">
			            <xs:annotation>
			                <xs:documentation>The unique identifier for this rail reservation</xs:documentation>
			            </xs:annotation>
			        </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>  
    
    <xs:element name="RailExchangeQuoteRsp">
        <xs:annotation>
            <xs:documentation>Returns the result of an availability search on host.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="RailSegmentList" minOccurs="0" />
                        <xs:element ref="RailJourneyList" minOccurs="0"/>
                        <xs:element ref="RailExchangeSolution" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="RailFareNoteList" minOccurs="0" />
                        <xs:element ref="RailFareIDList" minOccurs="0" />
                        <xs:element ref="RailFareList" minOccurs="0" />
                        <xs:element ref="common:HostTokenList" minOccurs="0" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailExchangeReq">
        <xs:annotation>
            <xs:documentation>Creates a rail exchange reservation request with the host.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseCreateReservationReq">
                    <xs:sequence>
                        <xs:element ref="RailExchangeSolution" minOccurs="0" maxOccurs="1"/> 
                        <xs:element ref="common:FormOfPayment" minOccurs="0" />
                        <xs:element ref="common:Payment" minOccurs="0" />
                        <xs:element ref="RailFareNoteList" minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>List of RailFareNote(s) that is referenced by key in RailFare.</xs:documentation>
                            </xs:annotation>
                        </xs:element> 
                        <xs:element ref="common:HostTokenList" minOccurs="0" />
                       <xs:element ref="RailAutoSeatAssignment" minOccurs="0" maxOccurs="unbounded"/> 
                    </xs:sequence>
	                <xs:attribute name="LocatorCode" type="common:typeLocatorCode" use="required">
		                <xs:annotation>
		                <xs:documentation>The unique identifier for this rail reservation</xs:documentation>
		                </xs:annotation>
	                </xs:attribute>     
				</xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="RailSeatMapReq">
        <xs:annotation>
            <xs:documentation>Request a rail seat map/coach map.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="RailInfo"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailSeatMapRsp">
        <xs:annotation>
            <xs:documentation>Returns rail seat map/coach map.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="Coach" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
      
    
    
</xs:schema>
