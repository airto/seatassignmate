            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="AirReservationLocatorCode" />
                        <xs:element ref="common:TicketNumber" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>All tickets that belong to this PNR must be enumerated here. Voiding only some tickets of a multi-ticket PNR not currently supported.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:attribute name="CancelAIR" type="xs:boolean" use="required">
                        <xs:annotation>
                            <xs:documentation>Value must be set to true. Currently must cancel reservation when voiding tickets.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
