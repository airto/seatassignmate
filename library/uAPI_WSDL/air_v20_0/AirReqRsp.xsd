			<xs:complexContent>
				<xs:extension base="common:BaseSearchRsp">
					<xs:sequence>
						<xs:element name="FlightTimeTableList" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="FlightTimeDetail" maxOccurs="unbounded" />
								</xs:sequence>
