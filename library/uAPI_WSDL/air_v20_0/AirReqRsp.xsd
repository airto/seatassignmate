    <xs:element name="AirCancelReq">
        <xs:annotation>
            <xs:documentation>Air Cancel request is used to cancel all or part of an AirReservation. Given only a locator code, everything within the AirReservation would be canceled. If particular segments are specified, then only those segments will be canceled.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:ContinuityCheckOverride" minOccurs="0" maxOccurs="1"/>
                        <xs:element ref="AirReservationLocatorCode" />
                        <xs:element ref="AirSegment" minOccurs="0" maxOccurs="unbounded" />
                        <xs:element ref="AirSegmentRef" minOccurs="0" maxOccurs="unbounded" />
                        <xs:element ref="common:FileFinishingInfo" minOccurs="0" />
                    </xs:sequence>
                    <xs:attribute name="Version" type="common:typeURVersion" use="required" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
