        <xs:annotation>
            <xs:documentation>Response to an AirCancelReq. AirReservation returned reflects the requested modifications.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:choice>
                        <xs:element ref="universalRecord_v18_0:UniversalRecord" minOccurs="0" maxOccurs="1" />
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
