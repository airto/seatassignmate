            <xs:annotation>
               <xs:documentation>Any vehicle - GDS Pseudo Code ACAR </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any2-3DoorVehicle">
            <xs:annotation>
               <xs:documentation>Any 2-3 door vehicle - GDS Pseudo Code ALLB </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any2-3DoorPassengerCar">
            <xs:annotation>
               <xs:documentation>Any 2-3 door passenger cars - GDS Pseudo Code ALBC </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any2-4DoorVehicle">
            <xs:annotation>
               <xs:documentation>Any 2-4 door vehicle - GDS Pseudo Code ALLC </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any2-4DoorPassengerCar">
            <xs:annotation>
               <xs:documentation>Any 2-4 door passenger cars - GDS Pseudo Code ALCC </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any4-5DoorVehicle">
            <xs:annotation>
               <xs:documentation>Any 4-5 door vehicle - GDS Pseudo Code ALLD </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
