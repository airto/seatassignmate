            <xs:annotation>
               <xs:documentation>Any 4 door passenger cars - GDS Pseudo Code ALDC </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyElite">
            <xs:annotation>
               <xs:documentation>Any elite - GDS Pseudo Code AELT </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyCoupe-Roadster">
            <xs:annotation>
               <xs:documentation>Any Coupe and/or Roadster - GDS Pseudo Code ACPR </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnySpecial">
            <xs:annotation>
               <xs:documentation>Any Special - GDS Pseudo Code ASPC </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyPickUp">
            <xs:annotation>
               <xs:documentation>Any PickUp - GDS Pseudo Code APUP </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyWagon">
            <xs:annotation>
               <xs:documentation>Any Wagon - GDS Pseudo Code AWGN </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
