<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/vehicle_v19_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.travelport.com/schema/vehicle_v19_0"
           xmlns:common="http://www.travelport.com/schema/common_v17_0"           
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../common_v17_0/CommonReqRsp.xsd" namespace="http://www.travelport.com/schema/common_v17_0"/>

    <xs:element name="VehicleReservation">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReservation">
                    <xs:sequence>
                        <xs:element ref="common:BookingTravelerRef" minOccurs="1" maxOccurs="unbounded"/>
                        <xs:element ref="common:ReservationName" minOccurs="0"/>
                        <xs:element ref="VehicleDateLocation"/>
                        <xs:element ref="Vehicle"/>
                        <xs:element ref="common:SpecialEquipment" minOccurs="0" maxOccurs="5"/>
                        <xs:element ref="VehicleSpecialRequest" minOccurs="0"/>
                        <xs:element ref="PaymentInformation" minOccurs="0"/>
                        <xs:element ref="DeliveryAddress" minOccurs="0">
							<xs:annotation>
								<xs:documentation>An address to which a rental car should be delivered and a phone number associated with the address.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="CollectionAddress" minOccurs="0">							<xs:annotation>
								<xs:documentation>An address from which a rental car should be picked up at the end of a rental period and a phone number associated with the address.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element ref="FlightArrivalInformation" minOccurs="0" >
 		                    <xs:annotation>
                    			<xs:documentation>
                    				The flight arrival information 
                    				(airline code and flight number) for the 
                    				airport/city at which the rental car will
                    				be picked up
                    			</xs:documentation>
                    		</xs:annotation>
                    	</xs:element>
						<xs:element ref="common:Guarantee" minOccurs="0"/>
						<xs:element ref="AssociatedRemark" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element ref="common:BookingSource" minOccurs="0">
                           <xs:annotation>
                              <xs:documentation>Specify alternate booking source</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                        <xs:element ref="common:ThirdPartyInformation" minOccurs="0"/>
                        <xs:element ref="common:SellMessage" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                    <xs:attribute name="SupplierCode" type="xs:string" use="required"/>
                    <xs:attribute name="BookingConfirmation" type="xs:string" use="optional"/>
                    <xs:attribute name="Status" type="xs:string" use="required"/>
                    <xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
                        <xs:annotation>
                            <xs:documentation>Provider reservation reference key.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PassiveProviderReservationInfoRef" type="common:typeRef" use="optional">
                        <xs:annotation>
                            <xs:documentation>Passive Provider reservation reference key.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="TravelOrder" type="xs:integer" use="optional">
                        <xs:annotation>
                            <xs:documentation>To identify the appropriate sequence for Air/Car/Hotel segments based on travel dates.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="PaymentInformation">
      <xs:annotation>
         <xs:documentation>The payment information for a vehicle reservation.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
             <xs:extension base="typePaymentInformation">
                  <xs:attribute name="Key" type="common:typeRef" use="optional"/>
              </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
    <xs:element name="VehicleDateLocation">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:VendorLocation" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="PickupDateTime" type="xs:string" use="required"/>
            <xs:attribute name="PickupLocation" type="common:typeIATACode" use="optional">
            	<xs:annotation>
                    <xs:documentation>PickUpLocation is optional if Reference point is specified</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ReturnDateTime" type="xs:string" use="required"/>
            <xs:attribute name="ReturnLocation" type="common:typeIATACode" use="optional">
                <xs:annotation>
                    <xs:documentation>If not specified, the PickupLocation will be assumed</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PickupLocationType" type="common:typeVehicleLocation" use="optional"/>
            <xs:attribute name="ReturnLocationType" type="common:typeVehicleLocation" use="optional"/>
            <xs:attribute name="PickupLocationNumber" type="xs:string" use="optional"/>
            <xs:attribute name="ReturnLocationNumber" type="xs:string" use="optional"/>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="Vehicle">
        <xs:annotation>
            <xs:documentation>Information related to single vehicle</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="VehicleRate" minOccurs="0"/>
                <!-- S1352-RateModifier Removed from Vehicle
                <xs:element ref="RateModifiers" minOccurs="0"/> -->
            </xs:sequence>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="required"/>
            <xs:attribute name="AirConditioning" type="xs:boolean" use="required"/>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="required"/>
            <xs:attribute name="VehicleClass" type="common:typeVehicleClass" use="required">
                <xs:annotation>
                    <xs:documentation>Class of vehicle</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Category" type="common:typeVehicleCategory" use="required">
                <xs:annotation>
                    <xs:documentation>Category of vehicle. Each supplier decides how these categories map to a vehicle class.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Description" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Car type Description such as 'CHRYSLER SEBRING OR SIMILAR'</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DoorCount" type="common:typeDoorCount" use="optional">
                <xs:annotation>
                    <xs:documentation>The number of doors on the vehicle. Could be a range like '2-4'</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Location" type="common:typeVehicleLocation" use="optional">
                <xs:annotation>
                    <xs:documentation>Location of the Vehicle or Counter</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VendorLocationKey" type="common:typeRef" use="optional">
                <xs:annotation>
                    <xs:documentation>Identifies the specific vendor location</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VendorName" use="optional">
                <xs:annotation>
                    <xs:documentation>The vendor's name</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="AlternateVendor" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>A vendor renting the vehicle on behalf of another company</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FuelType" type="common:typeFuel" use="optional">
	            <xs:annotation>
	               <xs:documentation>The fuel type of vehicle</xs:documentation>
	            </xs:annotation>
         	</xs:attribute>         	
			<xs:attribute name="Key" type="common:typeRef" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="VehicleRate">
        <xs:annotation>
            <xs:documentation>Rate summary</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="VehicleRateDescription" type="typeVehicleRateDescription" minOccurs="0"/>
                <xs:element name="RateHostIndicator" type="typeRateHostIndicator" minOccurs="0"/>
                <xs:element name="HourlyLateCharge" type="typeRateInfo" minOccurs="0"/>
                <xs:element name="DailyLateCharge" type="typeRateInfo" minOccurs="0"/>
                <xs:element ref="RateInclusions" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="EstimatedTotalAmount" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>The total amount</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RateForPeriod" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>The rate for the period</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RatePeriod" type="common:typeRateTimePeriod" use="optional">
                <xs:annotation>
                    <xs:documentation>The period for the rate (daily, weekly, etc)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="NumberOfPeriods" type="xs:int">
                <xs:annotation>
                  <xs:documentation>Define how many periods (e.g. number of days or weeks)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="UnlimitedMileage" type="xs:boolean" use="optional">
                <xs:annotation>
                    <xs:documentation>True if unlimited miles for rate</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MileageAllowance" type="xs:int" use="optional">
                <xs:annotation>
                    <xs:documentation>Only set if UnlimitedMileage is false. Number of miles allowed for rate</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExtraMileageCharge" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>Only set if UnlimitedMileage is false. Cost per mile over allowance for rate</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Units" type="common:typeDistance" use="optional">
                <xs:annotation>
                    <xs:documentation>Describes distance units for MileageAllowance or ExtraMileageCharge</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RateSource" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>The rate source indicator for GWS</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RateAvailability" type="typeRateAvailability" use="optional">
                <xs:annotation>
                    <xs:documentation>Rate is available to sell, Need to Call or Closed</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DropOffCharge" type="common:typeMoney" use="optional">
                <xs:annotation>
                    <xs:documentation>Additional amount charged when returning vehicle to a different location</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="BaseRate" type="common:typeMoney">
                <xs:annotation>
                    <xs:documentation>Rate for the entire rent without the required charges.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RequiredCharges" type="common:typeMoney"/>

            <!-- ==========S1532 - All attributes of RatePeriod moved to VehicleRate========== -->
            <xs:attribute name="RateCode" use="optional" type="common:typeRateCode"/>
            <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional">
                <xs:annotation>
                    <xs:documentation>The category of this rate (Best, etc)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DiscountNumber" use="optional" type="common:typeDiscountNumber"/>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="optional"/>
            <xs:attribute name="RateGuaranteed" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Guarantee indicator for vehicle rate.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RateCodePeriod" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>RateCodePeriod</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PromotionalCode" type="xs:string" use="optional">
    			<xs:annotation>
     				<xs:documentation>Specifies promotional code in vehicle book request. Returns applied promotional code in vehicle book response</xs:documentation>
    			</xs:annotation>
   			</xs:attribute>
   			<xs:attribute name="TourCode" type="xs:string" use="optional">
		    	<xs:annotation>
		     		<xs:documentation>Tour Number for the Vehicle Booking</xs:documentation>
		    	</xs:annotation>
		   	</xs:attribute>
		   	<xs:attribute name="RateGuaranteeType" type="common:typeRateGuarantee" use="optional">
                <xs:annotation>
                    <xs:documentation>To identify whether rate is already guaranteed or rate quoted or agent entered</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RequiredPayment" use="optional">
    			<xs:annotation>
					<xs:documentation>Returns Payment information required by vendor at the time og booking.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Guarantee" />
						<xs:enumeration value="Deposit" />
						<xs:enumeration value="PrePayment" />
					</xs:restriction>
				</xs:simpleType>
   			</xs:attribute>
            <!-- =================================================================================
-->
            <xs:attribute name="DropOffChargesIncluded" use="optional" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>If true: Drop off charges are included. If false, not included. If not specified, additional drop off charges MAY apply (but this has not been confirmed by vendor)</xs:documentation>
                </xs:annotation>
            </xs:attribute>            
        </xs:complexType>
    </xs:element>
    <xs:element name="RateModifiers">
        <xs:complexType>
			<xs:attribute name="RateCode" use="optional" type="common:typeRateCode"/>
            <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional">
                <xs:annotation>
                    <xs:documentation>The category of this rate (Best, etc)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
			<xs:attribute name="DiscountNumber" use="optional" type="common:typeDiscountNumber"/>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="optional"/>
            <xs:attribute name="RateGuaranteed" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Guarantee indicator for vehicle rate.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <!-- S1352 -->
            <xs:attribute name="RatePeriod" type="common:typeRateTimePeriod" use="optional">
                <xs:annotation>
                    <xs:documentation>The period for the rate code (daily, weekly, etc)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PromotionalCode" type="xs:string" use="optional">
    			<xs:annotation>
     				<xs:documentation>Specify promotional code used in Vehicle search request</xs:documentation>
    			</xs:annotation>
   			</xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:element name="VehicleSpecialRequest">
        <xs:annotation>
            <xs:documentation>Make a textual request to the Vehicle supplier.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="common:typeGeneralText">
                    <xs:attribute name="Key" type="common:typeRef" use="optional"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>

    <!--*** Shared reference elements ***-->
    <xs:element name="VehicleSearchModifiers">
      <xs:annotation>
         <xs:documentation>Controls and switches for the Vehicle Search request</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="PermittedVendors" minOccurs="0">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element ref="Vendor" maxOccurs="unbounded"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="ProhibitedVendors" minOccurs="0">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element ref="Vendor" maxOccurs="unbounded"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element ref="VehicleModifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="VehicleType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="RateModifiers" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="RateHostIndicator" type="typeRateHostIndicator" minOccurs="0"/>
            <xs:element ref="common:LoyaltyCard" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="common:ReferencePoint" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Search Car by reference point</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="Key" type="common:typeRef" use="optional"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="VehicleType">
      <xs:annotation>
         <xs:documentation>
         A standard list or classification of vehicle types .
         </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="typeVehicleTypes"/>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
