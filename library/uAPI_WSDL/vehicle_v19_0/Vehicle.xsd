               <xs:documentation>Any SUV - GDS Pseudo Code ASUV </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyPassengerVan">
            <xs:annotation>
               <xs:documentation> Any Passenger Van - GDS Pseudo Code AVAN</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any6PassengerVanOrSUV">
            <xs:annotation>
               <xs:documentation> Any 6 Passenger Van or SUV - GDS Pseudo Code ASIX</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any7PassengerVanOrSUV">
            <xs:annotation>
               <xs:documentation> Any 7 Passenger Van or SUV - GDS Pseudo Code ASEV</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any8PassengerVanOrSUV">
            <xs:annotation>
               <xs:documentation> Any 8 Passenger Van or SUV - GDS Pseudo Code AEIG</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Any4OrAllWheelDrive">
            <xs:annotation>
               <xs:documentation> Any 4 or all wheel drive- GDS Pseudo Code AFWD</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyAllTerrainVehicle">
            <xs:annotation>
               <xs:documentation> Any all Terrain Vehicle- GDS Pseudo Code ATRV</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyCommercialTruck">
            <xs:annotation>
               <xs:documentation> Any commercial truck- GDS Pseudo Code ACGO</xs:documentation>
