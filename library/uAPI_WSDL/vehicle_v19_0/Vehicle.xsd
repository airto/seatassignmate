            <xs:annotation>
               <xs:documentation> Any Crossover- GDS Pseudo Code ACRS</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllManualTransmissionVehicles">
            <xs:annotation>
               <xs:documentation> All Manual Transmission Vehicles- GDS Pseudo Code AMAN</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllAutomaticTransmissionVehicles">
            <xs:annotation>
               <xs:documentation> All Automatic Transmission Vehicles- GDS Pseudo Code AUTO</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllGasolineVehicles">
            <xs:annotation>
               <xs:documentation> All Gasoline Powered Vehicles- GDS Pseudo Code AGAS</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllPetrolVehicles">
            <xs:annotation>
               <xs:documentation> All Petrol Powered Vehicles- GDS Pseudo Code APET</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllDieselVehicles">
            <xs:annotation>
               <xs:documentation> All Diesel Powered Vehicles- GDS Pseudo Code ADSL</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnyGreenVehicle">
            <xs:annotation>
               <xs:documentation> Any Green vehicle (hybrid, electric, LPG, hydrogen, multi-fuel)- GDS Pseudo Code AGRN</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllHybridVehicles">
            <xs:annotation>
               <xs:documentation> All Hybrid Vehicles- GDS Pseudo Code AHYB</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllElectricVehicles">
            <xs:annotation>
               <xs:documentation>All Electric powered vehicles- GDS Pseudo Code AELC</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllHydrogenVehicles">
            <xs:annotation>
               <xs:documentation>All Hydrogen powered vehicles- GDS Pseudo Code AHYD</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllMultiFuelVehicles">
            <xs:annotation>
               <xs:documentation>All Multi-Fuel powered vehicles- GDS Pseudo Code AMFP</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllLPGVehicles">
            <xs:annotation>
               <xs:documentation>All LPG/Compressed Gas powered vehicles- GDS Pseudo Code ACPG</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AllEthanolVehicles">
            <xs:annotation>
               <xs:documentation>All Ethanol powered vehicles   - GDS Pseudo Code AETH</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
    </xs:simpleType>
    <xs:element name="VehicleModifier">
        <xs:complexType>
            <xs:attribute name="AirConditioning" type="xs:boolean" use="optional" default="true"/>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="optional" default="Automatic"/>
            <xs:attribute name="VehicleClass" type="common:typeVehicleClass" use="optional">
                <xs:annotation>
                    <xs:documentation>Class of vehicle</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Category" type="common:typeVehicleCategory" use="optional">
                <xs:annotation>
                    <xs:documentation>Category of vehicle. Each supplier decides how these categories map to a vehicle class.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DoorCount" type="common:typeDoorCount" use="optional">
                <xs:annotation>
                    <xs:documentation>The number of doors on the vehicle for a given category. Could be a range like '2-4'</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FuelType" type="common:typeFuel" use="optional">
	            <xs:annotation>
	               <xs:documentation>The fuel type of vehicle</xs:documentation>
	            </xs:annotation>
         	</xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="Vendor">
        <xs:annotation>
            <xs:documentation>The supplier code</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Code" type="common:typeSupplierCode" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="LocationInfo">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:LocationAddress" minOccurs="0" maxOccurs="5"/>
                <xs:element ref="common:PhoneNumber" minOccurs="0" maxOccurs="5"/>
                <xs:element ref="OperationTime" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element name="ShuttleInfo" type="xs:string" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="Name" type="xs:string" use="required">
                <xs:annotation>
                    <xs:documentation>A descriptive name (Los Angeles Intl Airport)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CounterLocation" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Where is the counter located</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="OperationTime" type="xs:string"/>
    <xs:element name="VehiclePolicy">
        <xs:annotation>
            <xs:documentation>Policy information provided by the Vehicle Supplier.  Usually relative to a specific location.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="VehicleDisclaimer" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="VehicleDetail" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="VendorInfo">
        <xs:complexType>
            <xs:attribute name="Code" type="common:typeSupplierCode" use="required"/>
            <xs:attribute name="Name" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="VehicleDetail">
    <xs:annotation>
        <xs:documentation>Make, model, etc information about the available vehicles</xs:documentation>
    </xs:annotation>
    <xs:complexType>
        <xs:attribute name="Code" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>The industry standard code for this vehicle</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SupplierCode" type="xs:string" use="optional">

            <xs:annotation>
                <xs:documentation>The supplier specific code for this vehicle.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="PassengerCount" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Number of passengers this car can hold (e.g. 4-5)</xs:documentation>
            </xs:annotation>

        </xs:attribute>
        <xs:attribute name="NumberOfDoors" type="xs:string">
            <xs:annotation>
                <xs:documentation>The number of doors (e.g. 2-4)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BagCount" type="xs:string">
            <xs:annotation>
                <xs:documentation>The number of bags</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Class" type="common:typeVehicleClass" use="required"/>
        <xs:attribute name="Category" type="common:typeVehicleCategory" use="required"/>
        <xs:attribute name="AirConditioning" type="xs:boolean" use="required"/>
        <xs:attribute name="Transmission" type="common:typeVehicleTransmission" use="required"/>
        <xs:attribute name="MakeModel" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation>The Make and Model description of this vehicle</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="FuelType" type="common:typeFuel" use="optional">
            <xs:annotation>
               <xs:documentation>The fuel type of vehicle</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    </xs:element>
    <xs:element name="VehicleDisclaimer">
        <xs:annotation>
            <xs:documentation>Textual information related to this rental location</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="Type" type="typeVehicleDisclaimer" use="required">
                        <xs:annotation>
                            <xs:documentation>The disclaimer category</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SubType" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation>Extra information about this category of disclaimer</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="Description" type="xs:string" use="optional">
                        <xs:annotation>
                            <xs:documentation>A verbal description of this disclaimer</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="typePickupDateLocation">
		<xs:annotation>
			<xs:documentation>A complexType for the pickup date, location, and location type.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Date" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>The start date for the vehicle rental.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		 <xs:attribute name="Location" type="common:typeIATACode" use="optional">
			<xs:annotation>
				<xs:documentation>The location (airport code) at which the vehicle will be picked up.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LocationType" type="common:typeVehicleLocation">
			<xs:annotation>
				 <xs:documentation>The type of location requested, such as resort, city center.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PickupLocationNumber" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Unique identifier for vehicle location</xs:documentation>
         </xs:annotation>
      </xs:attribute>
	</xs:complexType>
    <xs:element name="VehicleLocation">
        <xs:annotation>
            <xs:documentation>The information for a rental car location.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:VendorLocation" />
                <xs:element ref="common:Distance" minOccurs="0" />
                <xs:element ref="common:CoordinateLocation" minOccurs="0" />
                <xs:element ref="LocationInformation" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="LocationInformation">
        <xs:annotation>
            <xs:documentation>The type of location and the location address.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Address" type="common:typeStructuredAddress" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The address of the rental location.</xs:documentation>
                    </xs:annotation>
                </xs:element>    
            </xs:sequence>
             <xs:attribute name="LocationType" type="common:typeVehicleLocation">
                <xs:annotation>
                    <xs:documentation>Specifies the type of location, such as city center, airport, or near resort. </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="attrAreaInfo"/>
        </xs:complexType>
    </xs:element>
    <xs:attributeGroup name="attrAreaInfo">
		<xs:annotation>
			<xs:documentation>Holds area information for a group of vehicle
				location.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AreaGroup" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Unique area group code. like A, B etc.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Location" type="common:typeIATACode"
			use="optional">
			<xs:annotation>
				<xs:documentation>Location corresponding to the group.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AreaType" type="typeAreaInfo" use="optional">
			<xs:annotation>
				<xs:documentation>Location type corresponding to the group.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
    
    <xs:annotation>
        <xs:documentation>*** Vehicle specific types ***</xs:documentation>
    </xs:annotation>
    <xs:simpleType name="vehicleRateModifiers">
        <xs:annotation>
            <xs:documentation>Rate modifiers for the vehicle </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="G"/>
            <xs:enumeration value="Q"/>
            <xs:enumeration value="T"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="typeVehicleDisclaimer">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Vehicles"/>
            <xs:enumeration value="Shuttle"/>
            <xs:enumeration value="AgeRequirements"/>
            <xs:enumeration value="AddlDriverInfo"/>
            <xs:enumeration value="AddlDriverFees"/>
            <xs:enumeration value="Payment"/>
            <xs:enumeration value="Guarantee"/>
            <xs:enumeration value="Deposit"/>
            <xs:enumeration value="Voucher"/>
            <xs:enumeration value="License"/>
            <xs:enumeration value="Pickup"/>
            <xs:enumeration value="DropOff"/>
            <xs:enumeration value="Fuel"/>
            <xs:enumeration value="Geographic"/>
            <xs:enumeration value="Liabilities"/>
            <xs:enumeration value="SpecialEquipment"/>
            <xs:enumeration value="Insurance"/>
            <xs:enumeration value="Eligibility"/>
            <xs:enumeration value="Fees"/>
            <xs:enumeration value="Other"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="typeRateAvailability">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Available"/>
            <xs:enumeration value="Call"/>
            <xs:enumeration value="Closed"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="typeAreaInfo">
		<xs:annotation>
			<xs:documentation>The type of area category, such as AirportMain,
				AirportSecondary.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AirportMain" />
			<xs:enumeration value="AirportSecondary" />
			<xs:enumeration value="CityCenterDowntown" />
			<xs:enumeration value="EastOfCityCenter" />
			<xs:enumeration value="SouthOfCityCenter" />
			<xs:enumeration value="WestOfCityCenter" />
			<xs:enumeration value="NorthOfCityCenter" />
			<xs:enumeration value="PortOrFerry" />
			<xs:enumeration value="NearResort" />
			<xs:enumeration value="RailwayStation" />
		</xs:restriction>
	</xs:simpleType>
    
    <xs:complexType name="typePaymentInformation">
      <xs:sequence>
         <xs:element name="Voucher" type="typeVoucherInformation" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="BillingNumber" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>A Billing Number that may be associated to the Voucher.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="BillingReferenceNumber" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>A Number Assigned for Billing reconciliation processes that may also include a Corporate Account ID</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="PrePayment" use="optional">
  		<xs:annotation>
   			<xs:documentation>Amount paid in advance for  vehicle reservation. Can contain other non Money information to the vehicle supplier.</xs:documentation>
  		</xs:annotation>
  		<xs:simpleType>
   			<xs:restriction base="xs:string">
    			<xs:maxLength value="90"/>
   			</xs:restriction>
  		</xs:simpleType>
 	 </xs:attribute>
   </xs:complexType>
   <xs:complexType name="typeVoucherInformation">
      <xs:annotation>
         <xs:documentation>Information pertaining to the payment of a Vehicle Rental.</xs:documentation>
      </xs:annotation>
      <xs:attribute name="VoucherType" type="typeVoucherType" use="required">
         <xs:annotation>
            <xs:documentation>Specifies if the Voucher is for Full Credit or a Group/Day or a Monetary Amount.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Amount" type="common:typeMoney" use="optional">
         <xs:annotation>
            <xs:documentation>Amount associated with the Voucher.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ConfirmationNumber" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Confirmation from the vendor for the voucher</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="AccountName" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>Associated account name for the voucher</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
   <xs:simpleType name="typeVoucherType">
      <xs:restriction base="xs:string">
         <xs:enumeration value="FullCredit"/>
         <xs:enumeration value="GroupOrDay"/>
         <xs:enumeration value="SpecificValue"/>
      </xs:restriction>
   </xs:simpleType>
    <xs:complexType name="typeRateHostIndicator">
        <xs:annotation>
            <xs:documentation>Host-specific info for vendors</xs:documentation>
        </xs:annotation>
        <xs:attribute name="InventoryToken" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Vendor info about rate to adjust pricing as needed</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RateToken" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Assocates shop response to sell request</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="typeRateInfo">
        <xs:annotation>
            <xs:documentation>Additional information for extra days or hours.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="RateForPeriod" type="common:typeMoney" use="optional">
          <xs:annotation>
            <xs:documentation>The rate for the period</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="NumberOfPeriods" type="xs:int">
          <xs:annotation>
            <xs:documentation>Define how many periods (e.g. number of days or weeks)</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="UnlimitedMileage" type="xs:boolean" use="optional">
             <xs:annotation>
                 <xs:documentation>True if unlimited miles allowed. Not set if unknown</xs:documentation>
             </xs:annotation>
        </xs:attribute>
        <xs:attribute name="MileageAllowance" type="xs:int" use="optional">
          <xs:annotation>
            <xs:documentation>Number of miles or kilometers allowed for rate if not unlimited</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Units" type="common:typeDistance" use="optional">
          <xs:annotation>
            <xs:documentation>Describes distance units for MileageAllowance or ExtraMileageCharge</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ExtraMileageCharge" type="common:typeMoney" use="optional">
          <xs:annotation>
            <xs:documentation>Cost per mile or kilimeter over allowance for rate</xs:documentation>
          </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="typeRentalPeriod">
        <xs:annotation>
            <xs:documentation>Rental period information concerning minimum and max rental periods.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="RentalUnit" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">The rental unit period, such as days or hours.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Days"/>
                    <xs:enumeration value="Hours"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Length" type="xs:integer" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">The number of rental units.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RequirementPassed" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">If true, the rental period requirements have been met.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="typeDepositGuarantee">
        <xs:annotation>
            <xs:documentation>The deposit and guarantee information for a vehicle rental.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Purpose" type="common:typeReserveRequirement" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies if this is guarantee or deposit information.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Type" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies if deposit or guarantee rule is for a specified amount, the amount for a specific number of days or a percentage of the rental.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="NumberOfDays"/>
                    <xs:enumeration value="Amount"/>
                    <xs:enumeration value="Percent"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Amount" type="common:typeMoney" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The amount of money required for a guarantee or deposit.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Percent" type="xs:decimal" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The percentage of the rental amount for the guarantee or deposit.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DueDate" type="xs:date" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">The date a deposit is due.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="NumberOfDays" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the number of days from booking or prior to pickup by which the deposit should be made.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="OptionType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies if the number of days is after booking or prior to pickup.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="AfterBooking"/>
                    <xs:enumeration value="PriorToPickup"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Required" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">If true, this is required.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RequirementPassed" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">If true, the rental information passes the guarantee or deposit requirements.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="typeStartEndTime">
        <xs:annotation>
            <xs:documentation>Used to specify earliest and latest pickup/dropoff times for a vehicle.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="Time" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">The time in 24 hour clock format.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RequirementPassed" type="xs:boolean" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">When true, the time requirement has been met.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attributeGroup ref="common:attrDOW">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies the day(s) of the week to which the time requirement applies.</xs:documentation>
            </xs:annotation>
        </xs:attributeGroup>
    </xs:complexType>
    <xs:complexType name="typeVehicleCharge">
        <xs:annotation>
            <xs:documentation>Charges associated with a vehicle rental.</xs:documentation>
        </xs:annotation>
       	<xs:choice>
       		<xs:element name="Amount" type="common:typeMoney" minOccurs="0">
       			<xs:annotation>
               		<xs:documentation xml:lang="en">The amount of the charge.</xs:documentation>
           		</xs:annotation>
       		</xs:element>
       		<xs:element name="Percentage" type="common:typePercentageWithDecimal" minOccurs="0">
       			<xs:annotation>
       				<xs:documentation xml:lang="en">The amount of the charge in percentage.</xs:documentation>
       			</xs:annotation>
       		</xs:element>
       	</xs:choice>
        <xs:attribute name="Category" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="en">The type of charge information for the vehicle rental.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Tax"/>
                    <xs:enumeration value="Surcharge"/>
                    <xs:enumeration value="Coverage"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="Name" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Identifies the type of charge information for the category.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Type" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Used to specify how a charge is applied, such as per rental, per day, etc.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="NoCharge"/>
                    <xs:enumeration value="Percent"/>
                    <xs:enumeration value="Rental"/>
                    <xs:enumeration value="PerContract"/>
                    <xs:enumeration value="PerHour"/>
                    <xs:enumeration value="PerDay"/>
                    <xs:enumeration value="PerWeek"/>
                    <xs:enumeration value="PerMonth"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="IncludedInRate" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="en">Specifies whether the charge is included in the rate and if it is, if it is in the base or total rate.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="NotIncluded"/>
                    <xs:enumeration value="IncludedInBase"/>
                    <xs:enumeration value="IncludedInTotal"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="typeVehicleRateDescription">
        <xs:sequence>
            <xs:element name="Text" maxOccurs="unbounded">
                <xs:simpleType>
                    <xs:restriction base="xs:string"/>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="optional">
            <xs:annotation>
                <xs:documentation>Optional context name of the text block being returned i.e. Room details</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:element name="CollectionAddress">
		<xs:annotation>
			<xs:documentation>An address from which a rental car should be picked up at the end of a rental period and a phone number associated with the address.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:typeStructuredAddress">
					<xs:sequence>
						<xs:element ref="common:PhoneNumber" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="DeliveryAddress">
		<xs:annotation>
			<xs:documentation>An address to which a rental car should be delivered and a phone number associated with the address.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="common:typeStructuredAddress">
					<xs:sequence>
						<xs:element ref="common:PhoneNumber" minOccurs="0"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
    <xs:element name="FlightArrivalInformation">
		<xs:annotation>
			<xs:documentation>
				The flight arrival information
				(airline code and flight number) for the
				airport/city at which the rental car will
				be picked up
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="Carrier" type="common:typeCarrier"
				use="required">
				<xs:annotation>
					<xs:documentation>The carrier that is marketing this segment
					</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FlightNumber" >
				<xs:annotation>
					<xs:documentation>The flight number under which the marketing
						carrier is marketing this flight</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
		</xs:complexType>
	</xs:element>
	
   <xs:element name="VehiclePickupLocation">
      <xs:annotation>
         <xs:documentation>A container for Vehicle Location ,Vendor and Vehicle Modifier.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="Vendor"/>         
            <xs:element ref="VehicleModifier" minOccurs="0" maxOccurs="unbounded"/>
         </xs:sequence>
         <xs:attribute name="PickUpLocation" type="common:typeIATACode" use="required">
            <xs:annotation>
               <xs:documentation>The location (city or airport code) at which the vehicle will be picked up.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="VehicleSearchId">
      <xs:annotation>
         <xs:documentation>A container for Vehicle Media Links Search Id.</xs:documentation>
      </xs:annotation>
      <xs:complexType>         
         <xs:attribute name="MediaLinksSearchId" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>The search id  from VehicleSearchAvailabilityRsp to be used to retrieve the media links.</xs:documentation>
                    </xs:annotation>
           </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="VehicleWithMediaItems">
      <xs:annotation>
         <xs:documentation>A container for displaying vehicle details,media urls and errors</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="Vehicle"/>
            <xs:element ref="common:MediaItem" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Photos and other media urls for the item referenced above.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="MediaResultMessage" type="common:typeResultMessage" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Errors, Warnings and informational messages for the property referenced above.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="RateInclusions">
  		<xs:annotation>
   			<xs:documentation>Provides the list of additional charges included in Rate. e.g Tax, Airport Surcharge, CDW etc</xs:documentation>
  		</xs:annotation>
  		<xs:complexType>
   			<xs:sequence>
    			<xs:element ref="IncludedItem" maxOccurs="unbounded"/>
   			</xs:sequence>
  		</xs:complexType>
   </xs:element>
   <xs:element name="IncludedItem">
  		<xs:annotation>
   			<xs:documentation>Provides details of included item.</xs:documentation>
  		</xs:annotation>
  		<xs:complexType>
   			<xs:attribute name="Code" type="xs:string" use="required">
    			<xs:annotation>
     				<xs:documentation>Code for included item.</xs:documentation>
    			</xs:annotation>
   			</xs:attribute>
   			<xs:attribute name="Description" type="xs:string" use="required">
    			<xs:annotation>
     				<xs:documentation>Description of included item.</xs:documentation>
    			</xs:annotation>
   			</xs:attribute>
  		</xs:complexType>
   </xs:element>
   <xs:element name="VehicleRateInfo">
	  <xs:annotation>
		   <xs:documentation>Container for adding and modifying Vehicle Rate related information. 
		
				For Modify operation : 
				If a value is passed in any of the attributes they will be updated,
				If an empty string is passed then the attributed's value will be deleted,
				If the attribute is not passed no action will be taken.</xs:documentation>
		  </xs:annotation>
	  <xs:complexType>
		   <xs:attribute name="TourCode" type="xs:string" use="optional">
			    <xs:annotation>
			     	<xs:documentation>Tour Number for the Vehicle Booking</xs:documentation>
			    </xs:annotation>
		   </xs:attribute>
		   <xs:attribute name="DiscountNumber" type="common:typeDiscountNumber"	use="optional">
				<xs:annotation>
					<xs:documentation>Corporate Discount Number for the Vehicle	Booking</xs:documentation>
				</xs:annotation>
		   </xs:attribute>
		   <xs:attribute name="PromotionalCode" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Promotional Code for the Vehicle Booking</xs:documentation>
				</xs:annotation>
			</xs:attribute>
	  </xs:complexType>
 </xs:element>
  <xs:element name="AssociatedRemark">
     <xs:complexType>
        <xs:complexContent>
           <xs:extension base="common:typeAssociatedRemark"/>
        </xs:complexContent>
     </xs:complexType>
  </xs:element>

	<xs:element name="VehicleReturnDateLocation">
		<xs:annotation>
			<xs:documentation>Used to update Return Location and Return Date
				of existing Vehicle Booking.
				Modify operation : Only modification of ReturnDateTime is supported.
				Deletion of ReturnDateTime is not supported.
				If values passed in ReturnLocation, ReturnLocationType and
				ReturnLocationNumber are exactly same as PickupLocation,
				PickupLocationType and PickupLocationNumber of existing booking then
				ReturnLocation, ReturnLocationType and ReturnLocationNumber will be
				deleted.
				If values passed in ReturnLocation, ReturnLocationType and
				ReturnLocationNumber are not same as PickupLocation,
				PickupLocationType and PickupLocationNumber of existing booking then ReturnLocation,
				ReturnLocationType and ReturnLocationNumber will be updated.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="ReturnDateTime" type="xs:string" use="optional" />
			<xs:attribute name="ReturnLocation" type="common:typeIATACode" use="optional" />
			<xs:attribute name="ReturnLocationType" type="common:typeVehicleLocation" use="optional" />
			<xs:attribute name="ReturnLocationNumber" type="xs:string" use="optional" />
		</xs:complexType>
	</xs:element>
	<xs:element name="VehiclePickupDateLocation">
		<xs:annotation>
			<xs:documentation>Used to update PickupDateTime of existing Vehicle Booking.
                Modify operation : Only modification of PickupDateTime is supported.
                Deletion of PickupDateTime is not supported.            
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="PickupDateTime" type="xs:dateTime" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
